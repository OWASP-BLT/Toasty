# Example environment variables for Toasty Cloudflare Worker
# Copy this file to .env and fill in your values

# Cloudflare Account Configuration
# CLOUDFLARE_ACCOUNT_ID=your_account_id_here
# CLOUDFLARE_API_TOKEN=your_api_token_here

# Worker Configuration
ENVIRONMENT=development
LOG_LEVEL=info

# API Keys (use wrangler secret for production)
# API_KEY=your_api_key_here
# SECRET_KEY=your_secret_key_here

# Google GenAI Configuration (if integrating AI services)
# GOOGLE_API_KEY=your_google_api_key_here

# Database/Storage Configuration (if using KV, R2, or D1)
# KV_NAMESPACE_ID=your_kv_namespace_id_here
# R2_BUCKET_NAME=your_r2_bucket_name_here

# Monitoring and Analytics
# SENTRY_DSN=your_sentry_dsn_here
# ANALYTICS_TOKEN=your_analytics_token_here
